pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('test') {
            steps {
                echo 'testing'
            }
        }
        stage('release') {
            steps {
                echo 'releasing'
            }
        }
    }
}
